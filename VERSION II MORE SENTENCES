10 J = INT (6 * RND(1))+1
20 IF J = 1 THEN PRINT "ARE YOU OK":INPUT F$:GOTO 79
30 IF J = 2 THEN PRINT "DO YOU LIVE IN GERMANY": INPUT F$:GOTO 79
40 IF J = 3 THEN PRINT "DO YOU LIKE FOOTBALL": INPUT F$:GOTO 79
50 IF J = 4 THEN PRINT "DO YOU WANT A DRINK": INPUT F$:GOTO 79
60 IF J = 5 THEN PRINT "DO YOU WANT A SNAK": INPUT F$:GOTO 79
70 IF J = 6 THEN PRINT "ARE YOU TIRED": INPUT F$:GOTO 79
79 IF F$ = "I DONT KNOW" THEN PRINT "OH SORRY!"
80 IF F$ = "YES" THEN PRINT "OK"
81 IF F$ = "I KNOW" THEN PRINT "OK"
82 IF F$ = "SURE" THEN PRINT "YES"
83 IF F$ = "I CARE" THEN PRINT "OK"
84 IF F$ = "I WILL CATCH UP" THEN PRINT "YES FOR SURE"
85 IF F$ = "I DONT KNOW" THEN PRINT "OH SORRY!"
86 IF F$ = "I DONT CARE" THEN PRINT "REALLY?"
87 IF F$ = "I DONT WANT IT" THEN PRINT "NO PROBLEM"
10 J = INT (6 * RND(1))+1
20 IF J = 1 THEN PRINT "ARE YOU OK":INPUT F$:GOTO 79
30 IF J = 2 THEN PRINT "DO YOU LIVE IN GERMANY": INPUT F$:GOTO 79
40 IF J = 3 THEN PRINT "DO YOU LIKE FOOTBALL": INPUT F$:GOTO 79
50 IF J = 4 THEN PRINT "DO YOU WANT A DRINK": INPUT F$:GOTO 79
60 IF J = 5 THEN PRINT "DO YOU WANT A SNAK": INPUT F$:GOTO 79
70 IF J = 6 THEN PRINT "ARE YOU TIRED": INPUT F$:GOTO 79
79 IF F$ = "I M NOT SURE" THEN PRINT "OK, WE WILL SEE LATER" 
80 IF F$ = "YES" THEN PRINT "OK"
81 IF F$ = "I KNOW" THEN PRINT "OK"
82 IF F$ = "SURE" THEN PRINT "YES"
83 IF F$ = "I CARE" THEN PRINT "OK"
84 IF F$ = "I WILL CATCH UP" THEN PRINT "YES FOR SURE"
85 IF F$ = "I DONT KNOW" THEN PRINT "OH SORRY!"
86 IF F$ = "I DONT CARE" THEN PRINT "REALLY?"
87 IF F$ = "I DONT WANT IT" THEN PRINT "NO PROBLEM"
88 IF F$ = "NO" THEN PRINT "Ok!!" : CLEAR
100 GOTO 10
